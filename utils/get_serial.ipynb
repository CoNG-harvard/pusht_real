    "import pyrealsense2 as rs\n",
    "\n",
    "ctx = rs.context()\n",
    "devices = ctx.query_devices()\n",
    "print(\"Connected RealSense devices:\")\n",
    "\n",
    "for i, dev in enumerate(devices):\n",
    "    print(f\"Device {i}: {dev.get_info(rs.camera_info.serial_number)}\")\n"
   ]
